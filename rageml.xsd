<?xml version="1.0" ?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
		targetNamespace="http://markfrimston.co.uk/rageml"
		xmlns:rg="http://markfrimston.co.uk/rageml"
		elementFormDefault="qualified">

	<element name="comic">
		<complexType>
			<sequence minOccurs="1" maxOccurs="unbounded">
				<element ref="rg:panel" />
			</sequence>
			<anyAttribute />
		</complexType>
	</element>
	
	<element name="panel">
		<complexType>
			<sequence>
				<element ref="rg:narration" minOccurs="0" maxOccurs="1" />
				<group ref="rg:unordered-characters" minOccurs="0"/>
				<choice minOccurs="0" maxOccurs="unbounded">
					<choice>
						<element ref="rg:a-reply" />
						<element ref="rg:b-reply" />
						<element ref="rg:c-reply" />
						<element ref="rg:d-reply" />
						<element ref="rg:e-reply" />
					</choice>
					<group ref="rg:faces" />
				</choice>
				<element ref="rg:narration" minOccurs="0" maxOccurs="1" />
			</sequence>
			<anyAttribute />
		</complexType>
	</element>
	
	<element name="narration" type="rg:attr-token" />
	
	<element name="a" type="rg:character" />
	<element name="b" type="rg:character" />
	<element name="c" type="rg:character" />
	<element name="d" type="rg:character" />
	<element name="e" type="rg:character" />
	
	<element name="a-reply" type="rg:attr-token" />
	<element name="b-reply" type="rg:attr-token" />
	<element name="c-reply" type="rg:attr-token" />
	<element name="d-reply" type="rg:attr-token" />
	<element name="e-reply" type="rg:attr-token" />

	<element name="better-than-expected" type="rg:attr-token" />

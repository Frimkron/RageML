		xmlns:rg="http://markfrimston.co.uk/rageml"
		elementFormDefault="qualified">

	<element name="comic">
		<complexType>
			<complexContent>
				<restriction base="anyType">
					<sequence>
						<element ref="rg:panel" minOccurs="1" maxOccurs="unbounded" />
					</sequence>
					<anyAttribute />
				</restriction>
			</complexContent>
		</complexType>
	</element>
	
	<element name="panel">
		<complexType>
			<complexContent>
				<restriction base="anyType">
					<sequence>
						<element ref="rg:narration" minOccurs="0" maxOccurs="1" />
						<group ref="rg:unordered-characters" minOccurs="0"/>
						<choice minOccurs="0" maxOccurs="unbounded">
							<choice>
								<element ref="rg:a-reply" />
								<element ref="rg:b-reply" />
								<element ref="rg:c-reply" />
								<element ref="rg:d-reply" />
								<element ref="rg:e-reply" />
							</choice>
							<group ref="rg:faces" />
						</choice>
						<element ref="rg:narration" minOccurs="0" maxOccurs="1" />
					</sequence>
					<anyAttribute />
				</restriction>
			</complexContent>
		</complexType>
	</element>
	
	<element name="narration" type="rg:attr-token" />
	

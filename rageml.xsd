	<!-- TODO -->
	<elementt name="silent" type="token" />
	
	<!-- ====================================================== -->


	<complexType name="attr-token">
		<simpleContent>
			<extension base="token">
				<anyAttribute />
			</extension>
		</simpleContent>
	</complexType>
	
	<complexType name="character">
		<group ref="rg:faces" minOccurs="0" maxOccurs="1" />
		<attribute name="label" type="token" />
		<attribute name="sex">
			<simpleType>
				<restriction base="token">
					<pattern value="m|f" />
				</restriction>
			</simpleType>
		</attribute>
		<anyAttribute />
	</complexType>
	
	<group name="faces">
		<choice>
			<element ref="rg:derp" />
			<element ref="rg:trollface" />
			<element ref="rg:rage" />
			<element ref="rg:challenge-accepted" />
		</choice>
	</group>
	

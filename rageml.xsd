	<complexType name="face">
		<complexContent mixed="true">
			<restriction base="anyType">
				<sequence>
					<element ref="rg:silent" minOccurs="0" maxOccurs="1" />
				</sequence>
				<anyAttribute />
			</restriction>
		</complexContent>
